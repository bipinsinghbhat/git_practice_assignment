// prime number

let num=7;
for(let i=1;i<=num;i++){
if(num%i==0){
count++;
}}
if(count==2){
console.log("Prime")}
else{
console.log("Not prime");

// pallindrome

var name="racecar";
bag="";
for(let i=name.length-1;i>=0;i--){
 bag+"name[i]"; }

if(name=bag){
console.log("pallindrome");}
else{
console.log("not a pallindrome");
}